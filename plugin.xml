<?xml version="1.0" encoding="UTF-8"?>
<plugin title="BayInspect" loader="DEFAULT">
<overlays>
		<overlay id="startup.BayInspect" resourcebundle="content/BayInspect/resources/lang/messages">
			<menubar id="toolsmenu">
	        	<menuitem id="BayInspect" label="${Launcher.BayInspect}" command="mantleXulHandler.openUrl('${Launcher.BayInspect}','${Launcher.BayInspect_TOOLTIP}','plugin/BayInspect/api/default')"/>
	      	</menubar>
	    </overlay>
	</overlays>
	<lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
	<static-paths>
		<static-path url="/BayInspect/static" localFolder="static"/>
		<static-path url="/BayInspect/resources" localFolder="resources"/>
	</static-paths>
	<content-types>
		<content-type>
			<title>BayInspect</title>
			<description></description>
			<company name="" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
		</content-type>
	</content-types>
	<content-generator
		id="BayInspect"
		title="BayInspect"
		type="BayInspect"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		<!--<menu-item id="BayInspect_main" anchor="tools-submenu" label="BayInspect" command="content/BayInspect/" type="MENU_ITEM" how="LAST_CHILD"/>-->
	</menu-items>
</plugin>
